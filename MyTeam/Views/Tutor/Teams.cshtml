@model IEnumerable<MyTeam.Data.Team>

<div class="container body-content">

    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.User.FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.User1.FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.User2.FirstName)
