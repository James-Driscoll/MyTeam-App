                <td>
                    @Html.DisplayFor(modelItem => item.User7.FirstName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.User8.FirstName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.User9.FirstName)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.PK_TeamID }) |
                    @Html.ActionLink("Details", "Details", new { id = item.PK_TeamID }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.PK_TeamID })
                </td>
            </tr>
        }

    </table>
</div>