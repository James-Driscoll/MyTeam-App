            <th>
                @Html.DisplayNameFor(model => model.User6.FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.User7.FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.User8.FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.User9.FirstName)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.User.FirstName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.User1.FirstName)
                </td>
